  "description": "Mahr.io - Learning Management System",
  "scripts": {
    "start": "./node_modules/.bin/webpack --progress --profile --bail && node server.js",
    "build": "./node_modules/.bin/webpack --progress --profile --bail",
    "clean": "rm -rf ./node_modules && rm -rf ./bower_components && rm -rf ./public",
    "watch": "./node_modules/.bin/webpack --progress --profile --bail --watch",
    "server": "node server.js",
    "test": "npm run unit && npm run e2e",
    "coverage": "npm run unit && ./node_modules/.bin/http-server ./coverage/report/",
    "unit": "./node_modules/.bin/karma start --single-run",
    "tdd": "./node_modules/.bin/karma start",
    "e2e": "protractor ./protractor.conf.js",
    "lint": "./node_modules/.bin/eslint src",
    "postinstall": "./node_modules/.bin/bower install && npm run build"
  },
